{
  "name": "khs1994/khsci",
  "description": "CI Based Docker Powered by PHP",
  "keywords": [
    "ci",
    "docker"
  ],
  "homepage": "https://github.com/khs1994-php/khsci",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "khs1994",
      "email": "khs1994@khs1994.com",
      "homepage": "https://khs1994.com"
    }
  ],
  "support": {
    "issues": "https://github.com/khs1994-php/khsci/issues"
  },
  "require": {
    "php": ">=7.1",
    "pimple/pimple": "~3.0",
    "doctrine/cache": "^1.6",
    "khs1994/curl": "dev-master",
    "ext-json": "*"
  },
  "require-dev": {
    "ext-PDO": "*",
    "phpunit/phpunit": "^7.0"
  },
  "config": {
    "platform": {
      "php": "7.1.0"
    },
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "suggest": {
    "ext-xdebug": "*"
  },
  "autoload": {
    "psr-4": {
      "KhsCI\\": "src/KhsCI/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "KhsCI\\Tests\\": "tests/KhsCI/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "18.06-dev"
    }
  }
}
