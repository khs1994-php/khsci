{
  "name": "khs1994/pcit",
  "description": "A CI/CD System Based Docker Written by PHP",
  "keywords": [
    "ci",
    "docker"
  ],
  "homepage": "https://github.com/khs1994-php/pcit",
  "type": "project",
  "license": "MIT",
  "authors": [{
    "name": "khs1994",
    "email": "khs1994@khs1994.com",
    "homepage": "https://khs1994.com"
  }],
  "support": {
    "issues": "https://github.com/khs1994-php/pcit/issues"
  },
  "require": {
    "php": ">=7.1",
    "ext-PDO": "*",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-redis": "*",
    "doctrine/cache": "^1.6",
    "filp/whoops": "^2.3",
    "firebase/php-jwt": "^5.0",
    "khs1994/curl": "~18.06.0",
    "khs1994/docker": "~18.06.0",
    "khs1994/tencent-ai": "~18.06.0",
    "khs1994/wechat": "dev-master",
    "monolog/monolog": "^1.23",
    "nunomaduro/collision": "^2.0",
    "phpmailer/phpmailer": "^6.0",
    "pimple/pimple": "~3.0",
    "symfony/console": "^3.0|^4.0",
    "symfony/http-foundation": "^4.1",
    "symfony/var-dumper": "^3.0|^4.0",
    "symfony/yaml": "^3.0|^4.0",
    "vlucas/phpdotenv": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "phpunit/dbunit": "^4.0"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "suggest": {
    "ext-xdebug": "*",
    "ext-pcntl": "*"
  },
  "autoload": {
    "psr-4": {
      "PCIT\\": "src/PCIT/",
      "PCIT\\Deployer\\": "src/Deployer/",
      "PCIT\\Logs\\": "src/Logs/",
      "PCIT\\Api\\": "src/Api/",
      "App\\": "app/"
    },
    "files": [
      "src/PCIT/Kernel/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "PCIT\\Tests\\": "tests/PCIT/",
      "PCIT\\Tests\\Plugins\\": "tests/Plugins/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "18.06-dev"
    }
  }
}
